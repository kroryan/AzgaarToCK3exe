<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GraphicalUI.MainPage">

    <ContentPage.ToolbarItems>
        <ToolbarItem x:Name="loadButton" Text="Load" Clicked="loadButton_Clicked" />
        <ToolbarItem x:Name="saveButton" Text="Save" Clicked="saveButton_Clicked" />
        <ToolbarItem x:Name="createNewModButton" Text="Create New Mod" Clicked="createModButton_Clicked" />
        <ToolbarItem x:Name="populateModButton" Text="Populate Mod" Clicked="populateModButton_Clicked" />
        <ToolbarItem x:Name="settingsButton" Text="Settings" Clicked="settingsButton_Clicked" />
    </ContentPage.ToolbarItems>
    
    <WebView x:Name="webView" Source="https://pryvyd9.github.io/Fantasy-Map-Generator/" />
       
</ContentPage>

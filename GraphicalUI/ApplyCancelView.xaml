<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GraphicalUI.ApplyCancelView"
             Title="ApplyCancelView">
    <HorizontalStackLayout>
        <Button x:Name="applyButton" Text="Apply" Clicked="applyButton_Clicked" Margin="10,0"/>
        <Button x:Name="cancelButton" Text="Cancel" Clicked="cancelButton_Clicked" Margin="10,0"/>
    </HorizontalStackLayout>
</ContentPage>